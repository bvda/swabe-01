{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HealthChecksUI": {
    "HealthChecks": [{
      "Name": "Liveness",
      "Uri": "http://localhost:5000/live"
    }, {
      "Name": "Infrastructure",
      "Uri": "http://localhost:5000/infrastructure"
    }, {
      "Name": "Identity and Authorization",
      "Uri": "http://localhost:5000/users"
    }],
    "EvaluationTimeInSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  }
}
